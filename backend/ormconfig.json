{
  "type": "postgres",
  "host": "${DB_HOST}",
  "port": "${DB_PORT}",
  "username": "${DB_USERNAME}",
  "password": "${DB_PASSWORD}",
  "database": "${DB_DATABASE}",
  "synchronize": false,
  "logging": true,
  "entities": ["src/entity/**/*.ts"],
  "migrations": ["src/migration/**/*.ts"],
  "cli": {
    "migrationsDir": "src/migration"
  }
}
